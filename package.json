{
  "name": "rpa-flow-test",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:sidecar-winos": "pyinstaller -c -F --clean --name main-x86_64-pc-windows-msvc --distpath src-tauri/bin/api src-python/main.py",
    "build:sidecar-macos": "pyinstaller -c -F --clean --name main-aarch64-apple-darwin --distpath src-tauri/bin/api src-python/main.py",
    "build:sidecar-linux": "pyinstaller -c -F --clean --name main-x86_64-unknown-linux-gnu --distpath src-tauri/bin/api src-python/main.py",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix"
  },
  "dependencies": {
    "@fontsource/roboto": "5.2.5",
    "@mdi/font": "7.4.47",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-http": "~2",
    "@tauri-apps/plugin-shell": "~2",
    "vue": "^3.5.13",
    "vuetify": "^3.8.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.5.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "npm-run-all2": "^7.0.2",
    "pinia": "^3.0.1",
    "prettier": "3.5.3",
    "sass-embedded": "^1.86.3",
    "typescript": "~5.8.2",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-fonts": "^1.3.1",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.2.2",
    "vite-plugin-vue-layouts-next": "^0.0.8",
    "vite-plugin-vuetify": "^2.1.1",
    "vue-router": "^4.5.0",
    "vue-tsc": "^2.2.8"
  }
}
